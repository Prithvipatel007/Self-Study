   "source": [
    "# test for some random MxN matrix whether s (A+B) = sA + sB\n",
    "\n",
    "M = 5\n",
    "N = 3\n",
    "s = 5\n",
    "\n",
    "A = np.random.randn(M, N)\n",
    "B = np.random.randn(M, N)\n",
    "\n",
    "C = s * (A + B)\n",
    "D = (s*A) + (s*B)\n",
    "\n",
    "print(C), print(' '), print(D)\n",
    "\n",
    "print(D - C)"
   ]
